@model EF_Core_MVC_Code.Models.Yayinevleri

@{
    ViewData["Title"] = "Edit";
}

h1 class="display-3">Kitap Türü Düzenle</h1>
<hr>
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div class="form-group">
                <label class="control-label">Yayinevleri ID:</label>
                <label class="control-label" asp-for="Id">@Model.Id</label>
                <label class="control-label">Yayinevi </label>
                <input class="form-control" type="text" asp-for="Ad">
                  <label class="control-label">Adres </label>
                <input class="form-control" type="text" asp-for="Adres">
                  <label class="control-label">Telefon </label>
                <input class="form-control" type="text" asp-for="Tel">
            </div>
            <div class="form-group">
                <input type="submit" value="Değişiklikleri Kaydet" class="btn btn-primary">
            </div>
        </form>
    </div>
</div>