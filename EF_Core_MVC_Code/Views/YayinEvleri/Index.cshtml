@model List<EF_Core_MVC_Code.Models.Yayinevleri>
@{
    ViewData["Title"] = "Index";
}

<h1>YayinEvleri</h1>
<a asp-action="Create">Yeni YayınEvi EKLE></a>
<table class="table table-dark w-50">
    <thead>
        <tr>
            <th>YayinEvleri ID</th>
            <th>YayinEvleri Adı</th>
            <th>YayinEvleri Adresi</th>
            <th>YayinEvleri Telefon</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var yayinevleri in Model)
        {
            <tr>
                <td>@yayinevleri.Id</td>
                <td>@yayinevleri.Ad</td>
                <td>@yayinevleri.Adres</td>
                <td>@yayinevleri.Tel</td>
                <td>
                    <a class="btn btn-sm btn-primary" asp-action="Details" asp-route-id="@yayinevleri.Id">Detaylar</a>
                    <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-id="@yayinevleri.Id">Düzenle</a>
                    <a class="btn btn-sm btn-danger" asp-action="Delete" asp-route-id="@yayinevleri.Id">Sil</a>
                </td>
            </tr>
        }
    </tbody>
</table>